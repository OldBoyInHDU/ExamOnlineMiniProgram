/* pages/examresult/examresult.wxss */
.text-container {
    width: 100%;
    height: 200rpx;
    margin: 50rpx auto;
    line-height: 100px; /*设置line-height与父级元素的height相等*/
    text-align: center; /*设置文本水平居中*/
    overflow: hidden; /*防止内容超出容器或者产生自动换行*/

}

.result{
    width: 100%;
    height: 300rpx;
    margin: 10rpx auto;
    line-height: 50rpx; /*设置line-height与父级元素的height相等*/
    text-align: center; /*设置文本水平居中*/
  }

  .btn {
    display: flex;
    align-items: center;
    margin: 50rpx auto;
}